   "source": [
    "from threading import Thread\n",
    "import time\n",
    "x = 0\n",
    "\n",
    "def increment():\n",
    "    global x\n",
    "    time.sleep(0.001)\n",
    "    x = x + 1\n",
    "\n",
    "for i in range(100):\n",
    "    t = Thread(target=increment)\n",
    "    t.start()\n",
    "\n",
    "print(x)"
   ]
